local function isFromGitHub()
    -- Get the current script's source (works in most executors)
    local source = debug.getinfo(1, "S").source
    
    -- Check if it's running from your GitHub URL
    if source:find("https://raw.githubusercontent.com/JustSayy/Test/main/game") then
        return true
    end
    
    -- If source is "@" or "[string]", it's running directly (not from GitHub)
    if source == "@" or source == "[string]" then
        return false
    end
    
    -- Extra check for Synapse/Krnl (some executors modify the source)
    if source:match("%[string \".*%]") then
        return false
    end
    
    return false
end

if not isFromGitHub() then
    warn("ðŸš« This script must be loaded from:")
    warn("loadstring(game:HttpGet('https://raw.githubusercontent.com/JustSayy/Test/main/game'))()")
    return -- Stops execution if not from GitHub
end

-- âœ… If we reach here, it's running from your GitHub link
print("âœ… Authorized execution! Running script...")

